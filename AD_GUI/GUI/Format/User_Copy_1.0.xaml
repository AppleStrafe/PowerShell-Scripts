<Window x:Class="User_Copy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:User_Copy"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<DataGrid Name="Datagrid" AutoGenerateColumns="True" Margin="0,0,0,63" >
		</DataGrid>
		<Button x:Name="Copy_User" Content="Copy" HorizontalAlignment="Left" Margin="662,395,0,0" VerticalAlignment="Top" Background="#FF16B200" IsDefault="True" Width="128" Height="29" FontWeight="Bold" FontSize="18" IsEnabled="False"/>
		<Button Content="Cancel" HorizontalAlignment="Left" Margin="10,395,0,0" VerticalAlignment="Top" Background="#FFA70707" Width="128" Height="29" FontWeight="Bold" FontSize="18" IsCancel="True"/>
	</Grid>
</Window>
